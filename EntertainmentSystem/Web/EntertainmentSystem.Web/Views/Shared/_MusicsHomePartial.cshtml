@using EntertainmentSystem.Web.ViewModels.MediaContent
@model IEnumerable<MediaContentHomeViewModel>

<div id="carousel-ES-home-musics">

    @for (var i = 0; i < @Model.Count(); i++)
    {
        <div>
            <h3>@Model.ElementAt(i).Title</h3>
            <audio src="@Model.ElementAt(i).ContentUrl" controls></audio>
        </div>
    }
</div>
